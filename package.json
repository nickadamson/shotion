{
  "name": "shotion",
  "description": "Self-hosted GUI for RDBMS",
  "version": "0.1.0",
  "private": false,
  "prisma": {
    "schema": "db/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-sqlite": "node ./db/util/createSqliteDB.js",
    "create-postgres": "sudo -u postgres createdb shotion -p 5433 -O postgres",
    "psql-restart-cluster": "sudo pg_ctlcluster 12 main restart",
    "psql-connect": "sudo -u postgres psql shotion"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "better-sqlite3": "^7.5.3",
    "next": "12.1.6",
    "notion-types": "^6.12.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "prisma": "^3.15.2",
    "prisma-json-schema-generator": "^2.0.17",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickadamson/shotion/issues"
  },
  "homepage": "https://github.com/nickadamson/shotion#readme"
}
